{
  "version": 2,
  "builds": [
    { "src": "front-end", "use": "@vercel/static-build" },
    { "src": "back-end", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "back-end/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "front-end/index.html"
    }
  ]
}
